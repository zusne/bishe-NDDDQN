<?xml version="1.0"?>
<launch>
  <arg name="gui"        default="true"/>

 <!-- <arg name="world_file" default="$(find pioneer_utils)/gazebo/world/test_walking_people.world"/> -->
  <!--<arg name="world_file" default="$(find pioneer_utils)/gazebo/world/test_navigation.world"/> -->
  <arg name="world_file" default="$(find pioneer_utils)/gazebo/world/test_navigation.world"/>
  <include file="$(find gazebo_ros)/launch/empty_world.launch">
    <arg name="use_sim_time" value="true"/>
    <arg name="debug" value="false"/>
    <arg name="gui" value="$(arg gui)" />
    <arg name="world_name" value="$(arg world_file)"/>
  </include>

  <include file="$(find pioneer_utils)/gazebo/launch/base.launch.xml">
  </include>

  <node pkg="robot_state_publisher" type="robot_state_publisher" name="robot_state_publisher">
    <param name="publish_frequency" type="double" value="30.0" />
  </node> 


</launch>
